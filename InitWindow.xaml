<Window x:Class="STEM.InitWindow"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:local="clr-namespace:STEM"
		mc:Ignorable="d"
		Title="InitWindow" Height="450" Width="800" Loaded="InitWindow_loaded">
	<Window.Background>
		<SolidColorBrush>Black</SolidColorBrush>
	</Window.Background>
	<Grid>
		<Label x:Name="TitleLabel" Foreground="White" FontFamily="{StaticResource OpenSansBold}" FontSize="32" HorizontalAlignment="Center">start init...</Label>
	</Grid>
</Window>
