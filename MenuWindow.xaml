<Window x:Class="STEM.MenuWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:STEM"
        mc:Ignorable="d"
        AllowsTransparency="True" WindowStyle="None"
		WindowStartupLocation="CenterScreen" WindowState="Maximized"
		Title="MenuWindow" Width="1920" Height="1080">
	<Window.Background>
		<ImageBrush ImageSource="{StaticResource stemBackground}"></ImageBrush>
	</Window.Background>
    <Grid>
		<Label Foreground="White" FontFamily="{StaticResource OpenSansBold}" FontSize="32" HorizontalAlignment="Stretch" Margin="0,25,0,15">[MENU LOADED]</Label>
	</Grid>
</Window>
